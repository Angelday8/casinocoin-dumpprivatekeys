{
  "name": "casinocoin-dumpprivatekeys",
  "version": "1.0.0",
  "description": "Tool to dump all private keys found in a folder",
  "main": "index.js",
  "preferGlobal": true,
  "bin": "./index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && node ./dist/index.js",
    "ts-node": "npm run build && ts-node ./dist/index.ts",
    "build": "rm -rf dist && tsc",
    "binary": "npm run build && rm -rf release && mkdir release && node buildBinary.js"
  },
  "keywords": [
    "CSC",
    "Private",
    "Keys"
  ],
  "author": "andre@casinocoin.org",
  "license": "ISC",
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/inquirer": "0.0.38",
    "casinocoin-libjs-keypairs": "^1.0.0",
    "chalk": "^2.3.2",
    "commander": "^2.15.0",
    "core-js": "^2.5.3",
    "inquirer": "^5.1.0",
    "inquirer-path": "^1.0.0-beta5",
    "lodash": "^4.17.5",
    "ora": "^2.0.0",
    "typescript": "^2.7.2"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.46",
    "@types/ora": "^1.3.2",
    "nexe": "^2.0.0-rc.26"
  }
}
